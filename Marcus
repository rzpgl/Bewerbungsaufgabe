for(int i=0;i<theAlphabet.size();i++){
    bool doubleChar = true;
    for (n=0;n<=theAlphabet.size(),n++){
            if(theChars[n]==theAlphabet.get[i]){
                 System.Out.Println("Buchstabe schon vorhanden");
                 doubleChar=false;
                 break;
             }
     } 
     while(doubleChar){
           theChars[i]=theAlphabet.get[i];
     }
 }
